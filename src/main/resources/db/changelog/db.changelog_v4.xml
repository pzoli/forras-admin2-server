<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1765980224387-1"
               author="pzoli">
        <createTable tableName="media_info">
            <column autoIncrement="true"
                    name="id"
                    type="BIGINT">
                <constraints nullable="false"
                             primaryKey="true"
                             primaryKeyName="pk_media_info"/>
            </column>
            <column name="file_name"
                    type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="close_date"
                    type="DATETIME"/>
            <column name="system_user_sub"
                    type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1765980224387-2"
               author="pzoli">
        <addUniqueConstraint columnNames="file_name"
                             constraintName="uc_media_info_filename"
                             tableName="media_info"/>
    </changeSet>
    <changeSet id="1765980224387-3"
               author="pzoli">
        <createIndex indexName="idx_systemuser_sub"
                     tableName="media_info">
            <column name="system_user_sub"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>